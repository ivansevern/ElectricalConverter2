<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="60dp"
            android:layout_height="36dp"
            android:autoText="false"
            android:text="м"
            android:textAlignment="center"
            android:textSize="18sp"
            app:autoSizeTextType="none"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintStart_toEndOf="@+id/L"
            android:layout_marginTop="4dp" android:layout_marginStart="8dp" android:id="@+id/textView17"/>
    <TextView
            android:layout_width="21dp"
            android:layout_height="24dp"
            android:layout_marginStart="10dp"
            android:text="L"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" android:layout_marginTop="25dp"
            android:id="@+id/textView18"/>
    <EditText
            android:id="@+id/L"
            android:layout_width="169dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ввести длину"
            android:inputType="numberSigned"
            app:layout_constraintTop_toBottomOf="@+id/U" android:layout_marginStart="25dp"
            app:layout_constraintStart_toEndOf="@+id/textView18"/>
    <TextView
            android:id="@+id/textView12"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:text="кВт"
            android:textSize="18sp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14"
            android:layout_marginStart="10dp" app:layout_constraintStart_toEndOf="@id/PView"/>
    <Button
            android:id="@+id/button"
            android:layout_width="88dp"
            android:layout_height="0dp"
            android:text="Сброс"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="4dp" app:layout_constraintTop_toBottomOf="@+id/buttonP"
            android:layout_marginStart="8dp"/>
    <TextView
            android:id="@+id/PView"
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:textSize="18sp"
            android:layout_marginStart="10dp" app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/IView"
            android:layout_marginTop="30dp"/>
    <TextView
            android:id="@+id/textView11"
            android:layout_width="26dp"
            android:layout_height="0dp"
            android:text="P ="
            android:textSize="18sp"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/buttonP" android:layout_marginTop="31dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <Button
            android:text="ПЕРЕВОД В КВТ"
            android:layout_width="124dp"
            android:layout_height="52dp" android:id="@+id/buttonP"
            app:layout_constraintTop_toBottomOf="@+id/buttonI"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"/>
    <TextView
            android:id="@+id/textView5"
            android:layout_width="60dp"
            android:layout_height="36dp"
            android:autoText="false"
            android:text="Вольт"
            android:textAlignment="center"
            android:textSize="18sp"
            app:autoSizeTextType="none"
            app:layout_constraintTop_toBottomOf="@+id/textView10" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/U" android:layout_marginTop="4dp"/>
    <EditText
            android:id="@+id/I"
            android:layout_width="169dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ввести A"
            android:inputType="numberDecimal"
            app:layout_constraintTop_toBottomOf="@+id/P" android:layout_marginStart="25dp"
            app:layout_constraintStart_toEndOf="@+id/textView"/>
    <TextView
            android:id="@+id/textView"
            android:layout_width="21dp"
            android:layout_height="24dp"
            android:layout_marginStart="10dp"
            android:text="I "
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="25dp" app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <TextView
            android:id="@+id/textView10"
            android:layout_width="58dp"
            android:layout_height="43dp"
            android:autoText="false"
            android:text="Ампер"
            android:textAlignment="center"
            android:textSize="18sp"
            app:autoSizeTextType="none"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:layout_marginStart="11dp"
            app:layout_constraintStart_toEndOf="@+id/I" android:layout_marginTop="8dp"/>
    <EditText
            android:id="@+id/U"
            android:layout_width="169dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ввести Вольт"
            android:inputType="numberSigned"
            android:layout_marginStart="25dp"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/I"/>
    <TextView
            android:id="@+id/textView9"
            android:layout_width="21dp"
            android:layout_height="24dp"
            android:layout_marginStart="10dp"
            android:text="U"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="25dp" app:layout_constraintTop_toBottomOf="@+id/textView"
    />
    <TextView
            android:id="@+id/textView3"
            android:layout_width="46dp"
            android:layout_height="24dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="60dp"
            android:text="cosⴔ"
            android:textSize="18sp"
            app:layout_constraintEnd_toStartOf="@+id/cos"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="25dp"
            app:layout_constraintBottom_toTopOf="@+id/textView6"/>

    <TextView
            android:id="@+id/textView6"
            android:layout_width="21dp"
            android:layout_height="24dp"
            android:layout_marginStart="10dp"
            android:text="P ="
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginBottom="25dp" app:layout_constraintBottom_toTopOf="@+id/textView"/>

    <TextView
            android:id="@+id/textView8"
            android:layout_width="26dp"
            android:layout_height="23dp"
            android:text="I ="
            android:textSize="18sp"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/buttonI"
            app:layout_constraintTop_toBottomOf="@+id/L" android:layout_marginTop="20dp"/>
    <EditText
            android:id="@+id/cos"
            android:layout_width="169dp"
            android:layout_height="0dp"
            android:layout_marginTop="53dp"
            android:ems="10"
            android:hint="от 0,1 - 1"
            android:inputType="numberDecimal"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="parent"
    />

    <EditText
            android:id="@+id/P"
            android:layout_width="169dp"
            android:layout_height="48dp"
            android:layout_marginStart="25dp"
            android:ems="10"
            android:hint="ввести в кВт"
            android:inputType="numberSigned"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/cos"/>

    <TextView
            android:id="@+id/IView"
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:textSize="18sp"
            app:layout_constraintTop_toBottomOf="@+id/L" android:layout_marginTop="20dp"
            android:layout_marginStart="10dp" app:layout_constraintStart_toEndOf="@+id/textView8"/>
    <TextView
            android:id="@+id/textView14"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:text="Ампер"
            android:textSize="18sp"
            android:layout_marginStart="12dp" app:layout_constraintStart_toEndOf="@id/IView"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:layout_marginTop="20dp"/>
    <TextView
            android:id="@+id/textView4"
            android:layout_width="58dp"
            android:layout_height="43dp"
            android:layout_marginTop="20dp"
            android:autoText="false"
            android:text="кВт"
            android:textAlignment="center"
            android:textSize="18sp"
            app:autoSizeTextType="none"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginStart="11dp" app:layout_constraintStart_toEndOf="@+id/I"/>

    <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:text="ivansevern@gmail.com"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.505" app:layout_constraintTop_toBottomOf="@+id/button"
            android:layout_marginTop="10dp"/>
    <TextView
            android:id="@+id/textView7"
            android:layout_width="83dp"
            android:layout_height="33dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="60dp"
            android:autoText="true"
            android:text="     1 если     неизвестно"
            app:layout_constraintStart_toEndOf="@+id/cos"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="ПЕРЕВОД В А"
            android:layout_width="124dp"
            android:layout_height="46dp" android:id="@+id/buttonI"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/L" android:layout_marginTop="10dp"/>
    <Button
            android:text="HELP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/buttonHelp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="20dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>